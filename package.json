{
  "name": "tableau-sdk",
  "version": "0.4.0-hj",
  "description": "Create extracts and publish extracts to a Tableau Server using Node.js.",
  "main": "index.js",
  "dependencies": {
    "moment": "^2.14.1",
    "underscore": "^1.8.3",
    "node-pre-gyp" : "^0.6.x"
  },
  "bundledDependencies" : ["node-pre-gyp"],
  "devDependencies": {
    "chai": "^1.10.0",
    "chai-fs": "^0.1.0",
    "mocha": "^2.4.5"
  },
  "scripts": {
    "test": "mocha --retries 5 test/*.js",
    "preinstall" : "scripts/install-sdk.sh tableausdk",
    "install": "node-pre-gyp install --fallback-to-build"
  },
  "binary": {
    "module_name": "tableau",
    "module_path": "./lib/",
    "host": "https://node-lambda.s3.amazonaws.com",
    "remote_path" : "./{name}/v{version}"
  },
  "keywords": [
    "tableau",
    "extract",
    "api",
    "tde",
    "data"
  ],
  "author": {
    "name": "Tableau Marketing",
    "email": "",
    "url": "https://github.com/tableau-mkt"
  },
  "gypfile": true
}
